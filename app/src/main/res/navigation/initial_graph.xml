<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_nav"
    app:startDestination="@id/loginOperatorFragment">
    <fragment
        tools:layout="@layout/fragment_login"
        android:id="@+id/loginFragment"
        android:name="com.bits.bee.bpmc.ui.screen.login.LoginFragment"
        android:label="" >
        <action
            android:id="@+id/action_loginFragment_to_namaDeviceFragment"
            app:destination="@id/namaDeviceFragment" />
    </fragment>

    <fragment
        tools:layout="@layout/fragment_pilih_db"
        android:id="@+id/pilihDbFragment"
        android:name="com.bits.bee.bpmc.ui.screen.pilih_db.PilihDbFragment"
        android:label="" >
        <argument
            android:name="username"
            app:argType="string" />
        <argument
            android:name="password"
            app:argType="string" />
        <action
            android:id="@+id/action_pilihDbFragment_to_pilihCabangFragment"
            app:destination="@id/pilihCabangFragment" />
    </fragment>

    <fragment
        tools:layout="@layout/fragment_pilih_cabang"
        android:id="@+id/pilihCabangFragment"
        android:name="com.bits.bee.bpmc.ui.screen.pilih_cabang.PilihCabangFragment"
        android:label="" >
        <action
            android:id="@+id/action_pilihCabangFragment_to_pilihKasirFragment"
            app:destination="@id/pilihKasirFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_nama_device"
        android:id="@+id/namaDeviceFragment"
        android:name="com.bits.bee.bpmc.ui.screen.nama_device.NamaDeviceFragment"
        android:label="" >
        <argument
            android:name="username"
            app:argType="string" />
        <argument
            android:name="password"
            app:argType="string" />
        <action
            android:id="@+id/action_namaDeviceFragment_to_pilihDbFragment"
            app:destination="@id/pilihDbFragment" />
    </fragment>
    <dialog
        android:id="@+id/customDialogBuilder"
        android:name="com.bits.bee.bpmc.ui.dialog.CustomDialogBuilder"
        android:label="CustomDialogBuilder" />
    <action
        android:id="@+id/action_global_customDialogBuilder"
        app:destination="@id/customDialogBuilder" />
    <fragment
        tools:layout="@layout/fragment_pilih_kasir"
        android:id="@+id/pilihKasirFragment"
        android:name="com.bits.bee.bpmc.ui.screen.pilih_kasir.PilihKasirFragment"
        android:label="" >
        <action
            android:id="@+id/action_pilihKasirFragment_to_loginOperatorFragment"
            app:destination="@id/loginOperatorFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_login_pin"
        android:id="@+id/loginOperatorFragment"
        android:name="com.bits.bee.bpmc.ui.screen.login_operator.LoginOperatorFragment"
        android:label="" />

</navigation>