<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/berandaFragment">

    <fragment
        tools:layout="@layout/fragment_setting_list"
        android:id="@+id/settingListFragment"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_list.SettingListFragment"
        android:label="SettingListFragment" >
        <action
            android:id="@+id/action_settingListFragment_to_settingPosFragment"
            app:destination="@id/settingPosFragment" />
        <action
            android:id="@+id/action_settingListFragment_to_settingSistemFragment"
            app:destination="@id/settingSistemFragment" />
        <action
            android:id="@+id/action_settingListFragment_to_settingNotaFragment"
            app:destination="@id/settingNotaFragment" />
        <action
            android:id="@+id/action_settingListFragment_to_settingFavoriteFragmentMain"
            app:destination="@id/settingFavoriteFragmentMain" />
        <action
            android:id="@+id/action_settingListFragment_to_helpLicenseFragment"
            app:destination="@id/helpLicenseFragment" />
        <action
            android:id="@+id/action_settingListFragment_to_settingPrinterFragment"
            app:destination="@id/settingPrinterFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_setting_pos"
        android:id="@+id/settingPosFragment"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_pos.SettingPosFragment"
        android:label="SettingPosFragment" />
    <fragment
        android:id="@+id/berandaFragment"
        android:name="com.bits.bee.bpmc.presentation.ui.beranda.BerandaFragment"
        android:label="BerandaFragment" >
        <action
            android:id="@+id/action_berandaFragment_to_settingListFragment"
            app:destination="@id/settingListFragment" />
        <action
            android:id="@+id/action_berandaFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_berandaFragment_to_downloadActivity"
            app:destination="@id/downloadActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.bits.bee.bpmc.presentation.ui.pos.MainActivity"/>
    <activity
        android:id="@+id/downloadActivity"
        android:name="com.bits.bee.bpmc.presentation.ui.download.DownloadActivity"
        android:label="DownloadActivity" />

    <fragment
        android:id="@+id/settingSistemFragment"
        tools:layout="@layout/fragment_setting_sistem"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_sistem.SettingSistemFragment"
        android:label="SettingSistemFragment" />
    <fragment
        android:id="@+id/settingNotaFragment"
        tools:layout="@layout/fragment_setting_nota"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_nota.SettingNotaFragment"
        android:label="SettingNotaFragment" />
    <fragment
        tools:layout="@layout/fragment_setting_fav_main"
        android:id="@+id/settingFavoriteFragmentMain"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_favorite.SettingFavoriteFragmentMain"
        android:label="Favorite" />
    <fragment
        android:id="@+id/settingFavProdukFragment"
        tools:layout="@layout/fragment_setting_produk_fav"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_favorite.SettingFavProdukFragment"
        android:label="SettingFavProdukFragment" />
    <fragment
        android:id="@+id/helpLicenseFragment"
        tools:layout="@layout/fragment_help_lisensi"
        android:name="com.bits.bee.bpmc.presentation.ui.help_license.HelpLicenseFragment"
        android:label="HelpLicenseFragment" />
    <fragment
        android:id="@+id/settingPrinterFragment"
        tools:layout="@layout/fragment_setting_printer"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_printer.SettingPrinterFragment"
        android:label="SettingPrinterFragment" >
        <action
            android:id="@+id/action_settingPrinterFragment_to_addPrinterFragment"
            app:destination="@id/addPrinterFragment" />
    </fragment>
    <fragment
        android:id="@+id/addPrinterFragment"
        tools:layout="@layout/fragment_setting_add_printer"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_printer.add_printer.AddPrinterFragment"
        android:label="AddPrinterFragment" >
        <action
            android:id="@+id/action_addPrinterFragment_to_listPrinterFragment"
            app:destination="@id/listPrinterFragment" />
        <argument
            android:name="isNew"
            app:argType="boolean" />
        <argument
            android:name="printer"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/listPrinterFragment"
        tools:layout="@layout/fragment_setting_list_printer"
        android:name="com.bits.bee.bpmc.presentation.ui.setting_printer.list_printer.ListPrinterFragment"
        android:label="ListPrinterFragment" >
        <action
            android:id="@+id/action_listPrinterFragment_to_addPrinterFragment"
            app:destination="@id/addPrinterFragment" />
    </fragment>


</navigation>